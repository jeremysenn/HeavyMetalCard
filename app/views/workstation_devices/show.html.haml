- model_class = WorkstationDevice
.page-header
  %h1=t '.title', :default => model_class.model_name.human

%p
  %strong= 'WorkstationID:'
  %br
  = @workstation_device.WorkstationID
%p
  %strong= 'PinPadID:'
  %br
  = @workstation_device.PinPadID
%p
  %strong= 'ATMDevID:'
  %br
  = @workstation_device.ATMDevID
%p
  %strong= 'CompanyNbr:'
  %br
  = @workstation_device.CompanyNbr
%p
  %strong= 'IPAddr:'
  %br
  = @workstation_device.IPAddr

.form-actions
  = link_to t('.back', :default => t("helpers.links.back")), workstation_devices_path, :class => 'btn'
  = link_to t('.edit', :default => t("helpers.links.edit")), edit_workstation_device_path(@workstation_device), :class => 'btn'
  = link_to t('.destroy', :default => t("helpers.links.destroy")), workstation_device_path(@workstation_device), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
